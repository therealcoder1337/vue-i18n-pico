{
    "name": "vue-i18n-pico",
    "version": "0.5.1-typescript",
    "description": "Slim & fast i18n for Vue.js",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "scripts": {
        "lint": "eslint --ext .js,.ts src/ test/ index.ts",
        "fix": "eslint --ext .js,.ts --fix src/ test/ index.ts",
        "test": "ts-mocha -p test/tsconfig.json -n loader=ts-node/esm test/**/*.ts test/**/**/*.ts",
        "prepublishOnly": "tsc"
    },
    "exports": {
        ".": "./dist/index.js"
    },
    "keywords": [
        "vue",
        "vue.js",
        "vuejs",
        "i18n",
        "internationalization",
        "languages",
        "plugin"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/therealcoder1337/vue-i18n-pico.git"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.22.10",
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.5.0",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "eslint": "^8.47.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-import": "^2.28.0",
        "mocha": "^10.2.0",
        "should": "^13.2.3",
        "ts-mocha": "^10.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    },
    "peerDependencies": {
        "vue": "^3.3.4"
    }
}
